{
  "name": "idle-space2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "ng serve --open --prod",
    "build": "ng build --prod --base-href ./",
    "build:stats": "ng build --statsJson=true",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "make-themes2": "lessc --js ./src/themes/dark-green.less ./src/darkThemeGreen.css",
    "make-themes": "node --max-old-space-size=12288 ./src/makeThemes.js",
    "make-icons": "node ./src/makeIcons.js",
    "analyze": "webpack-bundle-analyzer dist/IdleSpace2/stats-es2015.json",
    "build:analyze": "npm run build:stats && npm run analyze",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "release": "ngh --dir=dist/IdleSpace2",
    "pretty": "prettier --write \"./**/*.{ts,js,jsx,json}\""
  },
  "private": true,
