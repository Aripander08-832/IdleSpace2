    "break_infinity.js": "^1.2.0",
    "date-fns": "^2.16.0",
    "json-2-csv": "^3.10.1",
    "less-plugin-clean-css": "^1.5.1",
    "lodash-es": "^4.17.20",
    "lz-string": "^1.4.4",
    "md5": "^2.3.0",
